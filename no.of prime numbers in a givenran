import java.util.*;
class primer
{
public static void main(String ars[])
{
Scanner sc=new Scanner(System.in);
System.out.println("Enter minimum range");
int mrang=sc.nextInt();
System.out.println("Enter maximum range");
int marang=sc.nextInt();
int count=0;
for(int i=mrang;i<=marang;i++)
{ 
int temp=0;
for(int j=2;j<i;j++)
{
if(i%j==0)
{
temp++;
}}
if(temp==0)
count++;
}
System.out.println("Number of prime numbers in a given range is "+count);
}}
