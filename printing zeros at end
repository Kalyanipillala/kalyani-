import java.util.*;
class arrayzero
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
System.out.print("Enter array size: ");
int size,i,temp=0;
size=sc.nextInt();
int arr[]=new int[size];
System.out.print("enter elements ");
for(i=0;i<size;i++)  
{
arr[i]=sc.nextInt();
}
for(i=0;i<size;i++)
{ 
 if(arr[i]!=0)
{
System.out.print(arr[i]+" ");
}
else
{
temp++;
}
}
for(i=0;i<temp;i++)
{ 
arr[i]=0;
System.out.print(arr[i]+" ");
}
}}


