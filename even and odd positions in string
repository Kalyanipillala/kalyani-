import java.util.*;
class strpos
{
public static void main(String[] args) 
{
String s;
Scanner sc=new Scanner(System.in);
System.out.println("enter the string");
s=sc.next();
int n=s.length();
System.out.print("odd: ");
for(int i=0;i<n;i++)
{
if(i%2==0)
System.out.print(s.charAt(i));
}
System.out.print("even: ");
for(int i=0;i<n;i++)
{        
if(i%2!=0)
System.out.print(s.charAt(i));
}
}
}
