import java.util.*;
class Factorial
{
int fact(int num)
{
if(num==1)
return 1;
else
return (num*fact(num-1));
}
}
class facttest
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
System.out.println("Enter number");
int num=sc.nextInt();
Factorial f= new Factorial();
System.out.println("Factorial of n: "+f.fact(num));
}
}
